{
  "docRootDir": ".",
  "docApiDir": "docs/apis",
  "docMarkdownDir": "docs/generated",

  "generateApi": false,

  "operation": "generate",
  "showInheritedMembers": false,
  "newlineKind": "crlf",

  "markdownOptions": {
    "fileLevel": "all",
    "indexFilename": "index",
    "indexTitle": "API Reference",
    "indexBreadcrumbTitle": "Home",
    "showPropertyDefaults": true,
    "hideEmptyTableColumns": true,
    "showBreadcrumb": true,
    "indexBreadcrumbUrl": "",
    "useIndex": true,
    "showRules": true,
    "showLineBreaks": true,
    "showCallSignatures": true,
    "collapseCallSignatures": true
  },

  "includePackageNames": [],
  "excludePackageNames": ["api-*"]
}

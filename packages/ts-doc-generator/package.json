{
  "name": "ts-doc-generator",
  "version": "1.0.0",
  "bin": {
    "ts-doc-generator": "./bin/ts-doc-generator"
  },
  "main": "lib/index.js",
  "typings": "dist/rollup.d.ts",
  "dependencies": {
    "@microsoft/api-extractor": "^7.33.4",
    "@microsoft/api-documenter": "^7.19.22",
    "colors": "~1.2.1",
    "js-yaml": "~3.13.1",
    "resolve": "~1.17.0"
  },
  "devDependencies": {
    "@types/js-yaml": "3.12.1",
    "@types/node": "12.20.24",
    "@types/resolve": "1.17.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "scripts": {
    "build": "tsc",
    "build:clean": "rimraf lib",
    "try": "--cwd ../../      /* TODO - these arent's quite working, it is hard to make it work with changed cwd */  ",
    "extract": "ts-node --cwd ../../ --project ./packages/ts-doc-generator/tsconfig.json ./packages/ts-doc-generator/src/main.ts e",
    "generate": "ts-node --cwd ../../ --project ./packages/ts-doc-generator/tsconfig.json ./packages/ts-doc-generator/src/main.ts g",
    "generate-old": "ts-node --cwd ../../ --project ./packages/ts-doc-generator/tsconfig.json ./packages/ts-doc-generator/src/main.ts go",
    "generate-new": "ts-node --cwd ../../ --project ./packages/ts-doc-generator/tsconfig.json ./packages/ts-doc-generator/src/main.ts gn",
    "docs": "npm run extract && npm run generate"
  }
}

{
  "private": "true",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "tsc --project packages/ts-doc-generator/tsconfig.json",
    "temp1": "--esm",
    "temp2": "--compiler-options={\"module\":\"commonJS\"}",
    "temp": "--compiler-options={\"moduleResolution\":\"nodenext\",\"module\":\"ESNext\"}",
    "extract": "ts-node --project packages/ts-doc-generator/tsconfig.json packages/ts-doc-generator/src/main.ts e",
    "generate": "ts-node --project packages/ts-doc-generator/tsconfig.json packages/ts-doc-generator/src/main.ts g",
    "generate-old": "ts-node --project packages/ts-doc-generator/tsconfig.json packages/ts-doc-generator/src/main.ts go",
    "generate-new": "ts-node --project packages/ts-doc-generator/tsconfig.json packages/ts-doc-generator/src/main.ts gn",
    "build-docs": "npm run extract && npm run generate",
    "prettier": "prettier --write ."
  },
  "devDependencies": {
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "ts-node": "^10.9.1"
  }
}
